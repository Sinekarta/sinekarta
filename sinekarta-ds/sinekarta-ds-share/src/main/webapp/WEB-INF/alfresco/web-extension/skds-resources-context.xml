<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>

	<!-- Load the messages into resourceBundle -->
	<bean id="skds.resourceBundles"
		class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent">
		<property name="resourceBundles">
			<list>
				<value>alfresco.messages.skds-actions</value>
				<value>alfresco.messages.skds-commons</value>
				<value>alfresco.messages.skds-pages</value>
			</list>
		</property>
	</bean>
	
</beans>
